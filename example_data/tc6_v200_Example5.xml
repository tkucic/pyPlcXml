                <body>
                  <ST>
                    <xhtml xmlns="http://www.w3.org/1999/xhtml">IF level1&lt;100 THEN
	full1:=FALSE; level1:=level1+0.1*rate1/100;
END_IF
IF level1&gt;=0 THEN
	level1:=level1-0.1*vrate1/100;
END_IF
IF level1&gt;=100 THEN
	level1:=100; full1:=TRUE;
END_IF

IF level2&lt;100 THEN
	full2:=FALSE; level2:=level2+0.1*rate2/100;
END_IF
IF level2&gt;=0 THEN
	level2:=level2-0.1*vrate2/100;
END_IF
IF level2&gt;=100 THEN
	level2:=100; full2:=TRUE;
END_IF

IF level3&lt;100 THEN
	full3:=FALSE; level3:=level3+0.1*rate3/100;
END_IF
IF level3&gt;=0 THEN
	level3:=level3-0.1*vrate3/100*1;  
END_IF
IF level3&gt;=100 THEN
	level3:=100; full3:=TRUE;
END_IF

IF level4&lt;100 THEN
	full4:=FALSE; level4:=level4+0.1*rate4/100;
END_IF
IF level4&gt;=0 THEN
	level4:=level4-0.1*vrate4/100;
END_IF
IF level4&gt;=100 THEN
	level4:=100; full4:=TRUE;
END_IF

level0:=100-(level1+level2+level3+level4)/4;
IF rate1&gt;0 AND (level0&gt;0 OR (valve1 OR valve2 OR valve3 OR valve4)) THEN pump1:=TRUE; ELSE pump1:=FALSE; END_IF
IF rate2&gt;0 AND (level0&gt;0 OR (valve1 OR valve2 OR valve3 OR valve4)) THEN pump2:=TRUE; ELSE pump2:=FALSE; END_IF
IF rate3&gt;0 AND (level0&gt;0 OR (valve1 OR valve2 OR valve3 OR valve4)) THEN pump3:=TRUE; ELSE pump3:=FALSE; END_IF
IF rate4&gt;0 AND (level0&gt;0 OR (valve1 OR valve2 OR valve3 OR valve4)) THEN pump4:=TRUE; ELSE pump4:=FALSE; END_IF

IF vrate1&gt;0 AND (level1&gt;0 OR pump1=TRUE) THEN valve1:=TRUE; ELSE valve1:=FALSE; END_IF
IF vrate2&gt;0 AND (level2&gt;0 OR pump2=TRUE) THEN valve2:=TRUE; ELSE valve2:=FALSE; END_IF
IF vrate3&gt;0 AND (level3&gt;0 OR pump3=TRUE) THEN valve3:=TRUE; ELSE valve3:=FALSE; END_IF
IF vrate4&gt;0 AND (level4&gt;0 OR pump4=TRUE) THEN valve4:=TRUE; ELSE valve4:=FALSE; END_IF
	
IF level0=0 THEN full1:=FALSE; full2:=FALSE; full3:=FALSE; full4:=FALSE; END_IF
</xhtml>
                  </ST>
                </body>
